{
  "name": "seaters",
  "version": "1.20.0",
  "description": "Seaters functionality in a box!",
  "main": "./dist/seaters.module.js",
  "typings": "./dist/index",
  "scripts": {
    "build": "node ./node_modules/del-cli/cli.js dist && node ./node_modules/webpack/bin/webpack.js -- --build && tsc -d --lib es6 --declarationDir dist/ src/index.ts && node ./node_modules/del-cli/cli.js lib",
    "serve": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base src/",
    "tslint": "./node_modules/tslint/bin/tslint --force --project tsconfig.json -t stylish --fix",
    "tslint-watch": "node ./node_modules/chokidar-cli **/*.ts -c \"npm run tslint\" --initial",
    "tslint-types": "./node_modules/tslint/bin/tslint --force --project tsconfig.json --type-check -t stylish --fix",
    "tslint-types-watch": "node ./node_modules/chokidar-cli **/*.ts -c \"npm run tslint-types\" --initial",
    "eslint": "node ./node_modules/eslint/bin/eslint.js e2e*/**/*.js --fix",
    "eslint-watch": "node ./node_modules/eslint-watch/bin/esw . -w --fix"
  },
  "author": "Ben Corne",
  "license": "ISC",
  "dependencies": {
    "@types/node": "7.0.28"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.3",
    "babel-preset-es2015": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "del-cli": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-config-strict": "^13.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-angular": "^2.3.0",
    "eslint-watch": "^3.1.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "string-replace-loader": "^1.2.0",
    "tslint": "^5.4.3",
    "tslint-config-standard": "^5.0.2",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0-rc.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}
